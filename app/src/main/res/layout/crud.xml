<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/idText"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:inputType="textCapCharacters"
            android:tag="@android:string/copyUrl"
            android:text="id" />

        <TextView
            android:id="@+id/userIdText"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="user_id" />

        <TextView
            android:id="@+id/namaText"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="nama" />

        <TextView
            android:id="@+id/jenis_kelaminText"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="jenis_kelamin"
            android:user="look data in input herself"/>

        <TextView
            android:id="@+id/TanggalLahiText"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="tanggal_lahir"
            android:api.format.json/>

        <TextView
            android:id="@+id/gambarText"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="gambar" />

        <TextView
            android:id="@+id/jabatanText"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="jabatan"
            android:request="serverapi"
            android:minChar="2"
            android:maxChar="50"
            />

        <TextView
            android:id="@+id/created_at"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:tag="@android:string/copyUrl"
            android:text="created_at" />

    </LinearLayout>

</LinearLayout>